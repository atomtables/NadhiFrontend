{
  "expo": {
    "name": "nadhi-frontend",
    "slug": "nadhi-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "nadhifrontend",
    "userInterfaceStyle": "automatic",
    "owner": "atomtables",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.nadhifrontend",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "This app uses the camera to take photos of flood conditions.",
        "NSLocationWhenInUseUsageDescription": "This app uses your location to tag photos with geographic data.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to save photos."
      },
      "config": {
          "googleMapsApiKey": "AIzaSyDQ0q81IbLrbny4iuw7DCyBEzjDUa5M4v4"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.nadhifrontend",
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "READ_MEDIA_IMAGES",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera to take photos of flood conditions.",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone for video recording.",
          "recordAudioAndroid": false
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos to save flood images.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos to your library.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
      "react-native-maps",
      {
        "iosGoogleMapsApiKey": "AIzaSyDQ0q81IbLrbny4iuw7DCyBEzjDUa5M4v4",
        "androidGoogleMapsApiKey": "AIzaSyDQ0q81IbLrbny4iuw7DCyBEzjDUa5M4v4"
      }
    ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Mozilla-Headline.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "324a7ee0-5da8-4335-b61c-4cb61b02e869"
      }
    }
  }
}
